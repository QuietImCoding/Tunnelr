{

  "description": "ICMP tunneling extension",
  "manifest_version": 2,
  "name": "TunnelR",
  "version": "1.0",
  "icons": {
    "48": "icons/off.svg"
  },

  "applications": {
    "gecko": {
      "id": "tunnelr@what.org",
      "strict_min_version": "50.0"
    }
  },

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icons/off.svg"
  },
 
  "permissions": ["nativeMessaging"]

}